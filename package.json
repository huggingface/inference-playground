		"lint": "prettier . --check . && eslint --ext .js,.ts,.svelte src/",
		"format": "prettier . --write ."
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.2.8",
		"@ryoppippi/unplugin-typia": "^1.0.0",
