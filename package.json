		"tslib": "^2.4.1",
		"typescript": "^5.8.2",
		"unplugin-icons": "^22.1.0",
		"vite": "^5.4.4"
	},
	"type": "module",
	"dependencies": {
		"@huggingface/hub": "^1.0.1",
		"@huggingface/inference": "^3.5.1",
		"@huggingface/tasks": "^0.17.1",
