{
	"name": "inference-playground",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "ts-patch install && svelte-kit sync || echo ''",
